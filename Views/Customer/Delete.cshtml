@model BankingMVC.Models.Dtos.CustomerDto
<div>Are you sure you want to delete this Customer?</div>
<div>
    <h4>@Model.FirstName</h4>
    <h4>@Model.MiddleName</h4>
    <h4>@Model.LastName</h4>
    <h4>@Model.Email</h4>
    <h4>@Model.PhoneNumber</h4>
    <h4>@Model.Address</h4>
</div>
<form asp-action="Delete">
    <input type="hidden" value="@Model.Id"/>
    <input class="btn btn-danger" type="submit" value="Delete"/>
    <li>
        <a asp-controller="Customer" asp-action="List">No</a>
        </li>
</form>